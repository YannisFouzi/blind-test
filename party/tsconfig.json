{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "lib": ["ES2021"],
    "types": ["@cloudflare/workers-types"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "target": "ES2021",
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
